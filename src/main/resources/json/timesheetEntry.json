{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "timesheetEntry",
  "title": "TimesheetEntry",
  "type": "object",
  "properties": {
    "date": {
      "type": "string",
      "format": "date"
    },
    "worked": {
      "type": "integer"
    },
    "internal_hours": {
      "type": "integer"
    },
    "courses": {
      "type": "integer"
    },
    "leave": {
      "type": "integer"
    },
    "special_leave": {
      "type": "integer"
    },
    "part_time_leave": {
      "type": "integer"
    },
    "sick": {
      "type": "integer"
    },
    "doctor": {
      "type": "integer"
    },
    "standby": {
      "type": "integer"
    }
  }
}